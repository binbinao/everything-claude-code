{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/",
    "docs/",
    "blog/",
    "*.ts",
    "*.tsx",
    "*.js"
  ],
  "exclude": [
    "node_modules/",
    "build/",
    ".docusaurus/"
  ]
}
